<div id="gitments"></div>
<script src="https://www.wenjunjiang.win/js/gitment.js"></script>
<script>
    var gitment = new Gitment({
      id: '<%= page.date%>',
      owner: '<%= owner%>',
      repo: '<%= repo%>',
      oauth: {
        client_id: '<%= client_id%>',
        client_secret: '<%= client_secret%>',
      },
    })
    gitment.render('gitments')
</script>
